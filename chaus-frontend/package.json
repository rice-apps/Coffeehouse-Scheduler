{
  "name": "starter",
  "version": "1.0.0",
  "description": "COMP 431/531 starter",
  "main": "./src/index.js",
  "scripts": {
    "clean": "rimraf dist/bundle.js*",
    "lint": "eslint src --ext .js --ext .jsx --cache",
    "watch": "webpack --watch",
    "build": "webpack -d",
    "deploy": "webpack -p && surge -p dist",
    "start": "webpack-dev-server --content-base dist -d --history-api-fallback",
    "test": "jest",
    "test:watch": "npm run test -- -w",
    "mocha": "mocha --opts mocha.opts src/**/*.spec.js",
    "karma": "karma start --single-run",
    "karma:watch": "karma start",
    "coverage": "isparta cover _mocha --config istanbul.yaml -- --opts mocha.opts src/**/*.spec.js",
    "e2e": "npm run e2e:base -- --reporter spec",
    "e2e:base": "mocha --opts e2e/mocha.opts e2e/*.spec.js",
    "e2e:watch": "npm run e2e:base -- --reporter spec -w",
    "e2e:xunit": "npm run e2e:base -- --reporter xunit > e2e/results.xml",
    "e2e:serve": "serve dist"
  },
  "author": "Scott Pollack",
  "engines": {
    "node": ">=6",
    "npm": ">=3"
  },
  "license": "MIT",
